<p id="notice"><%= notice %></p>

<h1>Infractors</h1>

<table  class="responsive-table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Numero de licencia</th>
      <th>Fecha de emision</th>
      <th>Fecha de vencimiento</th>
      <th>Sexo</th>
      <th>Nacionalidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infractors.each do |infractor| %>
      <tr>
        <td><%= infractor.nombre %></td>
        <td><%= infractor.apellido_paterno %></td>
        <td><%= infractor.apellido_materno %></td>
        <td><%= infractor.numero_de_licencia %></td>
        <td><%= infractor.fecha_de_emision %></td>
        <td><%= infractor.fecha_de_vencimiento %></td>
        <td><%= infractor.sexo %></td>
        <td><%= infractor.nacionalidad %></td>
        <td><%= link_to 'Show', infractor %></td>
        <td><%= link_to 'Edit', edit_infractor_path(infractor) %></td>
        <td><%= link_to 'Destroy', infractor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Infractor', new_infractor_path %>

<iframe src="https://www.google.com/maps" width="500" height="500" align="center"> </iframe>
<p id="notice"><%= notice %></p>
<h2>Lugares de trabajo</h2>
<table class="responsive-table">
  <thead>
  <tr>
    <th>Zona</th>
    <th>Direccion</th>
    <th>Descripcion</th>
  </tr>
  </thead>

  <tbody>
  <% @lugar_de_trabajo.each do |lugart| %>
    <tr>
      <td><%= lugart.zona %></td>
      <td><%= lugart.direccion %></td>
      <td><%= lugart.descripcion %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
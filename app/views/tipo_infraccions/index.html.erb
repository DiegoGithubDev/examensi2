<p id="notice"><%= notice %></p>

<h1>Tipo Infraccions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Monto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_infraccions.each do |tipo_infraccion| %>
      <tr>
        <td><%= tipo_infraccion.nombre %></td>
        <td><%= tipo_infraccion.monto %></td>
        <td><%= link_to 'Show', tipo_infraccion %></td>
        <td><%= link_to 'Edit', edit_tipo_infraccion_path(tipo_infraccion) %></td>
        <td><%= link_to 'Destroy', tipo_infraccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Infraccion', new_tipo_infraccion_path %>
